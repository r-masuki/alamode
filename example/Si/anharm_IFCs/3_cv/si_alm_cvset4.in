&general
  PREFIX = si222_cvset4
  MODE = optimize
  NAT = 64; NKD = 1
  KD = Si
/

&interaction
  NORDER = 3  # up to quartic
  NBODY = 2 3 3 
/

&cutoff 
  Si-Si None None 11.0
/

&optimize
  LMODEL = enet
  NDATA = 100
  DFSET = DFSET_randomQ
  FC2XML = ../2_generate_config/si222_harmonic.xml

  CV = -1  # manual CV
  L1_RATIO = 1.0 # LASSO
  CV_MINALPHA = 1.0e-7
  CV_MAXALPHA = 0.02
  CV_NALPHA = 30

  STANDARDIZE = 1
  CONV_TOL = 1.0e-8

  SKIP = 76-100
  NSTART_CV = 76
  NEND_CV = 100
/



&cell
  20.5262052 # factor in Bohr unit
  1.0 0.0 0.0 # a1
  0.0 1.0 0.0 # a2
  0.0 0.0 1.0 # a3
/



