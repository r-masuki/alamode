&general
  PREFIX = si222
  MODE = optimize
  NAT = 64; NKD = 1
  KD = Si
/

&interaction
  NORDER = 3  # up to quartic
  NBODY = 2 3 3 
/

&cutoff 
  Si-Si None None 11.0
/

&optimize
  LMODEL = enet
  NDATA = 100
  DFSET = ../3_cv/DFSET_randomQ
  FC2XML = ../2_generate_config/si222_harmonic.xml

  CV = 0 # switch off CV
  L1_RATIO = 1.0 # LASSO
  L1_ALPHA = 2.25633e-07

  CV_MINALPHA = 1.0e-7
  CV_MAXALPHA = 0.02
  CV_NALPHA = 30

  STANDARDIZE = 1
  CONV_TOL = 1.0e-9
/



&cell
  20.5262052 # factor in Bohr unit
  1.0 0.0 0.0 # a1
  0.0 1.0 0.0 # a2
  0.0 0.0 1.0 # a3
/


